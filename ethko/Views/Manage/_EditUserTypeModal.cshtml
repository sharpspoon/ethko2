@model ethko.UserType
@using (Html.BeginForm("EditSave", "Manage", new { @class = "form-horizontal", role = "form", UserTypeId = Model.UserTypeId }))
{
    <h3>User Type: <input name="NewUserType" class="form-control" type="text" placeholder=@Html.DisplayTextFor(m => m.UserTypeName) required></h3>
    @Html.AntiForgeryToken()
    <input type="submit" name="SaveButton" value="Save" class="btn btn-success" />
}